              <accessrestrict><p>c04 access restrict</p></accessrestrict>
            </c04>
            <!-- Level 4 unrestricted leaf -->
            <c04 id="4RRRX" level="file">
              <did><unittitle>4 - no self restriction</unittitle></did>
            </c04>
          </c03>
