    <dsc type="combined">
      <!--
        This is mostly a binary tree of components with and without their own restrictions, used
        to test the inheritance behavior. We want restrictions to cascade downward to any children that
        do not have their own restrictions, but also apply a generic restriction to any unrestricted node
        whose descendents all have restrictions. This latter case is only demonstrated by the final c02.
      -->

      <!-- Top level restricted branch -->
      <c01 id="1R" level="series">
        <did><unittitle>1 - self restriction</unittitle></did>
        <accessrestrict><p>c01 access restrict</p></accessrestrict>
        <!-- Level 2 restricted branch -->
        <c02 id="2RR" level="subseries">
          <did><unittitle>2 - self restriction</unittitle></did>
          <accessrestrict><p>c02 access restrict</p></accessrestrict>
          <!-- Level 3 restricted branch -->
          <c03 id="3RRR" level="file">
            <did><unittitle>3 - self restriction</unittitle></did>
            <accessrestrict><p>c03 access restrict</p></accessrestrict>
            <!-- Level 4 restricted leaf -->
